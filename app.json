{
  "expo": {
    "name": "OMYKI",
    "slug": "OMYKI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zaid.thewebsight.omyki",
      "useFrameworks": "static"
    },
    "android": {
      "package": "com.omyki.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.gif",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "omyki",
    "extra": {
      "eas": {
        "projectId": "7ea3ee0c-0af9-41fa-ba1c-5efb67cc0f63"
      }
    },
    "plugins": ["expo-image-picker", "expo-contacts"]
  }
}
